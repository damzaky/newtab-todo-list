{
  "name": "My custom new tab page",
  "description": "Overrides the new tab page",
  "version": "0.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
